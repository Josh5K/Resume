<div class="resume-container">
  <nav class="navbar navbar-expand-lg navbar-dark bg-primary fixed-top" id="sideNav">
    <a class="navbar-brand" href="#">
      <img class="img-fluid img-profile rounded-circle mx-auto mb-2" src="<%= @person.image %>" alt="">
    </a>
    <div class"information">
      <h4><%= @person.name %></h4>
      <p>
      <%= @person.address %>
      <br/>
      (<%= @person.phone_number.to_s[0...3] %>) <%= @person.phone_number.to_s[3...6] %>-<%= @person.phone_number.to_s[6...100] %>
      <br/>
      <%= @person.email %>
      </p>
    </div>
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav">
      <% @person.links.each do |link| %>
      <li class="nav-item">
        <a class="nav-link" href="<%= link.source %>"><%= link.key %></a>
      </li>
      <% end %>
    </ul>
    </div>
  </nav>
  <div class="grid" >
    <div class="left">
      <h1>Experience</h1>
    </div
    <div class="right">
      <h1>Education</h1>
    </div>
  </div>
</div>